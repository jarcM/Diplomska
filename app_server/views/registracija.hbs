
<center> <h1> Obrazec za registracijo </h1> </center>
<script src="https://www.google.com/recaptcha/api.js" async defer></script>
<style>
    input:focus, textarea:focus, select:focus{
        outline: none;
    }
</style>

<form id="dodajUporabnik" action ="" method="post" role="form" onSubmit="return ValidateEmail(this)&&ValidatePassword(this)&&checkPassword(this)&&ValidateUsername(this)">
    <div class="container">
        <label>Elektronski naslov</label>
            <svg xmlns="http://www.w3.org/2000/svg" id="checkEmail" color="#76d600"  display="none" width="16" height="16" fill="currentColor" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
            </svg>
            <svg xmlns="http://www.w3.org/2000/svg" id="xEmail" color="red" display="none" width="16" height="16" fill="currentColor" class="bi bi-x-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z"/>
            </svg>
        <input type="text" placeholder="Vnesite elektronski naslov" name="email" id="email" onKeyup="SprotnoPreverjanjeEmail()" required>
        <label>Uporabniško ime </label>
            <svg xmlns="http://www.w3.org/2000/svg" id="checkUser" color="#76d600"  display="none" width="16" height="16" fill="currentColor" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
            </svg>
            <svg xmlns="http://www.w3.org/2000/svg" id="xUser" color="red" display="none" width="16" height="16" fill="currentColor" class="bi bi-x-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z"/>
            </svg>
        <input  type="text" placeholder="Vnesite željeno uporabniško ime" name="username" id="username" onKeyup="SprotnoPreverjanjeUpIme()" required
                title="Uporabniško ime mora vsebovati med 8 in 15 veljavnih znakov. Šumniki so prepovedani.">
        <br>
        <label>Geslo</label>
            <svg xmlns="http://www.w3.org/2000/svg" id="checkPassword" color="#76d600"  display="none" width="16" height="16" fill="currentColor" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
            </svg>
            <svg xmlns="http://www.w3.org/2000/svg" id="xPassword" color="red" display="none" width="16" height="16" fill="currentColor" class="bi bi-x-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z"/>
            </svg>
        <input  type="password" id="password" placeholder="Vnesite geslo" name="password" onKeyup="SprotnoPreverjanjeGeslo()"required
                title="Geslo mora vsebovati vsaj eno številko, eno malo črko, eno veliko črko, en poseben znak in biti dolžine vsaj 8 znakov">

        <label>Ponovno vnesite geslo </label>
            <svg xmlns="http://www.w3.org/2000/svg" id="checkPassword2" color="#76d600"  display="none" width="16" height="16" fill="currentColor" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
            </svg>
            <svg xmlns="http://www.w3.org/2000/svg" id="xPassword2" color="red" display="none" width="16" height="16" fill="currentColor" class="bi bi-x-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z"/>
            </svg>
        <input  type="password" id="password2" placeholder="Vnesite geslo" name="password2" onKeyup="SprotnoPreverjanjePonovitevGesla()" required>
        <div class="container">
            <div class="row form-group">
                <div class="col-xs-12-col-md-4 nejcRows">
                    <label>Rojstni datum </label>
                    <input id="datum" type="date" class="form-control" name="datum">
                </div>
            </div>
        </div>
        <div class="col-xs-12-col-md-6">
            <div class="form-group">
                <label>Izberi tip oglasa</label>
                <div class="form-check">
                    <label class="form-check-label">
                        <input id="vloga" type="radio" class="form-check-input" name="vloga" value="lastnik" >Lastnik psa
                    </label>
                </div>
                <div class="form-check">
                    <label class="form-check-label">
                        <input id="vloga" type="radio" class="form-check-input" name="vloga" value="sprehajalec" >Sprehajalec psa
                    </label>
                </div>
                <div class="form-check">
                    <label class="form-check-label">
                        <input id="vloga" type="radio" class="form-check-input" name="vloga" value="skrbnik" >Skrbnik psa
                    </label>
                </div>

            </div>
        </div>
        <br>
        <form action="?" method="POST">
            <div class="g-recaptcha" data-sitekey="6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI"></div>
            <br/>
        </form>
        <input type="checkbox" required > Strinjam se s
        <a href="/splosnipogoji">splošnimi pogoji registracije. </a>
        <br>
        <button id=register> Registriraj se</button>
        <br>
        Že imaš račun?
        <a href="/prijava">Prijavi se </a>
    </div>
</form>


