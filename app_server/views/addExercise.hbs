<form id="dodajVaje" action="" method="post" role="form">
    <div class="container">
        <div class="container">
            <div class="collection_text">Add exercise</div>
        </div>
        <div class="container bg-light2 flex-wrapper">
            <div class="container">
                <div class="racing_shoes">
                    <div class="form-group">
                        <label>Exercise name:</label>
                        <input style="background-color: #EBEBEB" id="naslov" type="text" class="form-control" name="naslov" required>
                    </div>
                    <div class="form-group">
                        <label>Main body part</label>
                        <input style="background-color: #EBEBEB" id="bodyPart" type="text" class="form-control" name="bodyPart" required>
                    </div>
                    <div class="form-group" style="text-align:center">
                        <button style="align-items: center"class="btn btn-primary" id="uredi">Add exercise</button>
                        <button href="/myEvents" type="button" id="izbrisi" class="btn btn-light">Finish</button>
                    </div>
                </div>
            </div>
        </div>

    </div>
</form>
